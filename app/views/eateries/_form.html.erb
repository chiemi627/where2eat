<%= form_with model: eatery, local:true do |f| %>
    <div class='.form-group'>
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control", id: 'eatery_name' %>        
    </div>
    <div class='.form-group'>
        <%= f.label :address %>
        <%= f.text_field :address, class: "form-control", id: 'eatery_address' %>        
    </div>
    <div class='.form-group'>
        <%= f.label :tel %>
        <%= f.text_field :tel, class: "form-control", id: 'eatery_tel' %>        
    </div>
    <div class='.form-group'>
        <%= f.label :regular_holiday %>
        <%= f.text_field :regular_holiday, class: "form-control", id: 'eatery_regular_holiday' %>        
    </div>
    <div class='.form-group'>
        <%= f.label :open %>
        <%= f.text_field :open, class: "form-control", id: 'eatery_open' %>        
    </div>
    <div class='.form-group'>
        <%= f.label :close %>
        <%= f.text_field :close, class: "form-control", id: 'eatery_close' %>        
    </div>

    <%= f.submit nil, class: 'btn btn-primary' %>
<% end %>